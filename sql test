ALTER TABLE Vehicles ALTER COLUMN id TYPE serial;

create table user (
id SERIAL PRIMARY KEY,
make TEXT,
model TEXT,
year INTEGER,
ownerId INTEGER
);

CREATE TABLE users (
id SERIAL PRIMARY KEY,
firstname TEXT,
lastname TEXT,
email TEXT
);
